{
  "manifest_version": 3,
  "name": "GeoGeoGeo",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.geoguessr.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["interceptor.js"],
    "matches": ["*://*.geoguessr.com/*"]
  }]
}