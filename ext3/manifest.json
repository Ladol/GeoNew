{
    "manifest_version": 3,
    "name": "Network Monitor",
    "version": "1.0",
    "permissions": [
      "storage",
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "web_accessible_resources": [{
      "resources": ["interceptor.js"],
      "matches": ["<all_urls>"]
    }]
  }